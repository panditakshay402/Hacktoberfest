#include<iostream>
using namespace std;
int main()
{
	int n,b,c;
	cout<<"enter the subject ";
	cin>>n;
	cout<<"enter the marks ";
	int a[n];
	for(int i=0;i<n;i++){
		cin>>a[i];
		
	}
	b=a[0];
	c=a[0];
	
	
	
	for(int i=0;i<n;i++){
		
		if(a[i]>b){
			b=a[i];
		}
			if(a[i]<c){
			c=a[i];
		}
		
	//	cout<<a[i]<<endl;
		
	}
	
	cout<<c<<"\n"<<b;
}
